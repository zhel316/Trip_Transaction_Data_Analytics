{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tripdatafactory"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"TripTransaction_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TripTransaction'"
		},
		"reward_points_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'reward_points'"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureSql_ridetranTable1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "trip_id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "trip_start_timestamp",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "trip_end_timestamp",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "driver_id",
						"type": "nvarchar"
					},
					{
						"name": "driver_name",
						"type": "nvarchar"
					},
					{
						"name": "source_location_address1",
						"type": "nvarchar"
					},
					{
						"name": "source_city",
						"type": "nvarchar"
					},
					{
						"name": "source_province_state",
						"type": "nvarchar"
					},
					{
						"name": "source_country",
						"type": "nvarchar"
					},
					{
						"name": "destination_location_address1",
						"type": "nvarchar"
					},
					{
						"name": "destination_city",
						"type": "nvarchar"
					},
					{
						"name": "destination_province_state",
						"type": "nvarchar"
					},
					{
						"name": "destination_country",
						"type": "nvarchar"
					},
					{
						"name": "total_distance",
						"type": "int",
						"precision": 10
					},
					{
						"name": "total_fare",
						"type": "int",
						"precision": 10
					},
					{
						"name": "trip_status",
						"type": "nvarchar"
					},
					{
						"name": "delay_start_time_mins",
						"type": "int",
						"precision": 10
					},
					{
						"name": "payment_method",
						"type": "nvarchar"
					},
					{
						"name": "payment_Status",
						"type": "nvarchar"
					},
					{
						"name": "customer_id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "customer_name",
						"type": "nvarchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "trip_transactions_1"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureSqlDatabase1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlDatabase1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('AzureSqlDatabase1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/triptransactionfact')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "AzureSql_ridetranTable1",
								"type": "DatasetReference"
							},
							"name": "datasource1"
						}
					],
					"sinks": [
						{
							"linkedService": {
								"referenceName": "TripTransaction",
								"type": "LinkedServiceReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [
						{
							"name": "filter1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          trip_id as integer,",
						"          trip_start_timestamp as timestamp,",
						"          trip_end_timestamp as timestamp,",
						"          driver_id as string,",
						"          driver_name as string,",
						"          source_location_address1 as string,",
						"          source_city as string,",
						"          source_province_state as string,",
						"          source_country as string,",
						"          destination_location_address1 as string,",
						"          destination_city as string,",
						"          destination_province_state as string,",
						"          destination_country as string,",
						"          total_distance as integer,",
						"          total_fare as integer,",
						"          trip_status as string,",
						"          delay_start_time_mins as integer,",
						"          payment_method as string,",
						"          payment_Status as string,",
						"          customer_id as integer,",
						"          customer_name as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> datasource1",
						"datasource1 filter(1 == 1) ~> filter1",
						"filter1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     format: 'delta',",
						"     container: 'deltalake',",
						"     folderPath: 'Bronze/trip_trans',",
						"     mergeSchema: false,",
						"     autoCompact: false,",
						"     optimizedWrite: false,",
						"     vacuum: 0,",
						"     deletable: false,",
						"     insertable: true,",
						"     updateable: false,",
						"     upsertable: false,",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     saveOrder: 1) ~> sink1"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureSql_ridetranTable1')]",
				"[concat(variables('factoryId'), '/linkedServices/TripTransaction')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/TripTransaction')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('TripTransaction_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Data flow1",
						"type": "ExecuteDataFlow",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataflow": {
								"referenceName": "triptransactionfact",
								"type": "DataFlowReference",
								"parameters": {},
								"datasetParameters": {
									"datasource1": {},
									"sink1": {}
								}
							},
							"staging": {},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/dataflows/triptransactionfact')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSql_rewardTable1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "trip_id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "customer_rating",
						"type": "int",
						"precision": 10
					},
					{
						"name": "driver_rating",
						"type": "int",
						"precision": 10
					},
					{
						"name": "customer_id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "driver_id",
						"type": "nvarchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "ride_ratings1"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureSqlDatabase1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/rewardpointsfact')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "AzureSql_rewardTable1",
								"type": "DatasetReference"
							},
							"name": "source1"
						}
					],
					"sinks": [
						{
							"linkedService": {
								"referenceName": "reward_points",
								"type": "LinkedServiceReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [],
					"scriptLines": [
						"source(output(",
						"          trip_id as integer,",
						"          customer_rating as integer,",
						"          driver_rating as integer,",
						"          customer_id as integer,",
						"          driver_id as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> source1",
						"source1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     format: 'delta',",
						"     container: 'deltalake',",
						"     folderPath: 'Bronze/reward_point',",
						"     mergeSchema: false,",
						"     autoCompact: false,",
						"     optimizedWrite: false,",
						"     vacuum: 0,",
						"     deletable: false,",
						"     insertable: true,",
						"     updateable: false,",
						"     upsertable: false,",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> sink1"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureSql_rewardTable1')]",
				"[concat(variables('factoryId'), '/linkedServices/reward_points')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/reward_points')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('reward_points_connectionString')]"
				}
			},
			"dependsOn": []
		}
	]
}